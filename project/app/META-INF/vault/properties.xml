<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>FileVault Package Properties</comment>
	<entry key="createdBy">admin</entry>
    <entry key="cqVersion">${cq.version}</entry>
    <entry key="groupId">${project.groupId}</entry>
    <entry key="artifactId">${project.artifactId}</entry>
    <entry key="name">${project.artifactId}</entry>
    <entry key="description">${project.description}</entry>
    <entry key="version">${project.version}</entry>
    <entry key="requiresRoot">true</entry>
    <entry key="lastWrapped">${cq.lastWrapped}</entry>
    <entry key="lastWrappedBy">${env.USER}</entry>
    <entry key="buildCount">${buildNumber}</entry>
    <entry key="acHandling">overwrite</entry>
    <entry key="group">consulting</entry>
	<entry key="dependencies"/>
	<entry key="packageFormatVersion">2</entry>	
	<entry key="path">/etc/packages/my_project</entry>
</properties>
